                    <uib-tab index="1" active="dashboard.avatarTab" id="avatar" heading="Avatar">

                        <form class="clearfix" name="avatarForm" enctype="multipart/form-data">
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-12">
                                        <label>
                                            Upload New Avatar
                                        </label>
                                        <!-- custom file upload -->
                                        <div class="fancy-file-upload fancy-file-primary">
                                            <i class="fa fa-upload"></i>
                                            <input id="fileInput" type="file" onchange="angular.element(this).scope().readProfile(this.files[0]); jQuery(this).next('input').val(this.value);">
                                            <input type="text" class="form-control" placeholder="no file selected" readonly="" />
                                            <span class="button">Choose File</span>
                                        </div>
                                        <small class="text-muted block">Max file size: 10Mb (zip/pdf/jpg/png)</small>
                                        <div class="row">
                                            <div ng-if="dashboard.dataUrl" class="img-container col-md-6">
                                                <div class="box-static margin-top-15">
                                                    <div class="box-icon-title">
                                                        <h5 class="margin-bottom-0">Crop</h5>
                                                    </div>
                                                    <img ng-if="dashboard.dataUrl"
                                                         ng-src="{{dashboard.dataUrl}}"
                                                         width="300"
                                                         height="300"
                                                         ng-cropper
                                                         ng-cropper-proxy="dashboard.cropperProxy"
                                                         ng-cropper-show="dashboard.showEvent"
                                                         ng-cropper-hide="dashboard.hideEvent"
                                                         ng-cropper-options="dashboard.options">
                                                </div>
                                            </div>
                                            <div ng-if="dashboard.preview.dataUrl" class="preview-container col-md-6">
                                                <div class="box-static margin-top-15">
                                                    <div class="box-icon-title">
                                                        <h5 class="margin-bottom-0">Preview</h5>
                                                    </div>
                                                    <div class="thumbnail text-center">
                                                        <img id="filePreviewImage" ng-src="{{dashboard.preview.dataUrl}}" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row form-group col-md-12">
                                            <button class="btn btn-default" ng-click="dashboard.preview(); $event.preventDefault();">Crop</button>
                                            <button class="btn btn-primary" ng-if="dashboard.dataUrl" ng-click="dashboard.upload('Profile'); $event.preventDefault();"><i class="fa fa-upload"></i>Upload</button>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </form>

                    </uib-tab>
