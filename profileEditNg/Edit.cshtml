@model Sabio.Web.Models.ViewModels.BaseViewModel
@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_LayoutSmarty.cshtml";
}

<base href="/" />
<link href="~/Content/ngCropper.all.css" rel="stylesheet" />

<style>
    .img-container {
        max-width: 300px;
    }


    .title {
        opacity: 0;
        transition: opacity .5s ease-in;
        height: 100%;
        width: 100%;
        background-color: rgba(0,0,0,.5);
        position: relative;
        top: 0;
        text-align: center;
        display: table;
    }

        .title span {
            display: table-cell;
            vertical-align: bottom;
            height: 100%;
            width: 100%;
        }

    .thumbnail:hover .title {
        opacity: 1;
    }
</style>

<div class="container" ng-controller="personController as dashboard">
    <section class="page-header page-header-xs">
        <div class="container">

            <!-- breadcrumbs -->
            <ol class="breadcrumb breadcrumb-inverse">
                <li><a href="#">Home</a></li>
                <li><a href="#">Pages</a></li>
                <li class="active">{{dashboard.item.firstName}} {{dashboard.item.lastName}}</li>
            </ol><!-- /breadcrumbs -->

        </div>
    </section>
