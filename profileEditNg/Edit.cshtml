                    <div class="title" ng-click="dashboard.show('agentTab')"><span>Click to Change Logo</span></div>

                    <h2 class="size-18 margin-top-10 margin-bottom-0">{{dashboard.item.companyName}}</h2>
                </div>
                <div class="box-light margin-bottom-30" ng-hide="dashboard.isAgent || dashboard.isBroker || dashboard.item.roles.length > 0">
                    <!-- .box-light OR .box-light -->
                    <div class="row margin-bottom-20">
                        <div class="text-center">
                            <a href="" ng-click="dashboard.show('serviceProvider')">Are you a Service Provider? <br />Click here to Register</a>
                        </div>
                    </div>

                </div>
                <div class="box-light margin-bottom-30" ng-show="dashboard.isBroker && dashboard.agentSettings.isOfficer && !dashboard.agentSettings.registeredOffice">
                    <!-- .box-light OR .box-light -->
                    <div class="row margin-bottom-20">
                        <div class="text-center">
                            <a href="" ng-click="dashboard.show('registerOffice')">Would you like to register your Office?<br />Click here to Register</a>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </section>
</div>


